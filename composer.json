{
    "name": "davidpiesse/nova-toggle",
    "description": "A Laravel Nova Toggle field.",
    "keywords": [
        "laravel",
        "nova",
        "toggle",
        "field"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Davidpiesse\\NovaToggle\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Davidpiesse\\NovaToggle\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
